{
  "name": "Homefront",
  "version": "1.0.0",
  "description": "A Simple SPA built with vue and graphql",
  "author": "Rizki Ardian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf public/css public/js",
    "dev": "webpack-dev-server --config webpack.config.babel.js --open --inline --hot",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.babel.js --progress --hide-modules"
  },
  "dependencies": {
    "apollo-client": "^1.2.2",
    "axios": "^0.16.1",
    "date-fns": "1.28.5",
    "marked": "^0.3.6",
    "normalize.css": "7.0.0",
    "prismjs": "^1.6.0",
    "vue": "^2.3.3",
    "vue-apollo": "^2.1.0-beta.8",
    "vue-disqus": "^2.0.3",
    "vue-router": "2.5.3"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "^6.22.0",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.5",
    "pug": "2.0.0-rc.1",
    "pug-loader": "^2.3.0",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "0.4.3",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "2.1.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "12.1.0",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.3.3",
    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-server": "2.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "comments": false
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 5 versions"
        ]
      }
    }
  }
}
